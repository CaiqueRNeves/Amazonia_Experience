# Configurações do ambiente de desenvolvimento
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_NAME=AmazôniaExperience (Dev)
REACT_APP_VERSION=0.1.0

# Configurações de autenticação
REACT_APP_AUTH_STORAGE_KEY=amazonia_auth_dev
REACT_APP_TOKEN_REFRESH_INTERVAL=1800000

# Chaves de API para serviços externos
REACT_APP_MAPBOX_TOKEN=your_mapbox_dev_token_here
REACT_APP_LEAFLET_ACCESS_TOKEN=your_leaflet_dev_token_here

# PWA
REACT_APP_CACHE_VERSION=dev
REACT_APP_OFFLINE_ENABLED=true

# Internacionalização
REACT_APP_DEFAULT_LANGUAGE=pt-BR
REACT_APP_SUPPORTED_LANGUAGES=pt-BR,en-US,es-ES,fr-FR,de-DE,zh-CN,ru-RU

# Recursos da aplicação
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_CHATBOT=true
REACT_APP_ENABLE_QUIZ=true

# Configurações de desenvolvimento
REACT_APP_DEBUG=true
REACT_APP_MOCK_API=false
REACT_APP_MOCK_GEOLOCATION=false
REACT_APP_DEFAULT_LATITUDE=-1.455833
REACT_APP_DEFAULT_LONGITUDE=-48.503889
REACT_APP_DISABLE_AUTHENTICATION=false

# Ferramentas de desenvolvimento
BROWSER=none
HTTPS=true
PORT=8080